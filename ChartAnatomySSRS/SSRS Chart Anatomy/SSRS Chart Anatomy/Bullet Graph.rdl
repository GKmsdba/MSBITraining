                      <UniqueName>[Date].[Calendar Year].[Calendar Year]</UniqueName>
                    </ID>
                    <ItemCaption>Calendar Year</ItemCaption>
                    <UniqueName>true</UniqueName>
                  </Item>
                  <Item>
                    <ID xsi:type="Measure">
                      <MeasureName>Sales Actual</MeasureName>
                      <UniqueName>[Measures].[Sales Actual]</UniqueName>
                    </ID>
                    <ItemCaption>Sales Actual</ItemCaption>
                    <BackColor>true</BackColor>
                    <ForeColor>true</ForeColor>
                    <FontFamily>true</FontFamily>
                    <FontSize>true</FontSize>
                    <FontWeight>true</FontWeight>
                    <FontStyle>true</FontStyle>
                    <FontDecoration>true</FontDecoration>
                    <FormattedValue>true</FormattedValue>
                    <FormatString>true</FormatString>
                  </Item>
                  <Item>
                    <ID xsi:type="Measure">
                      <MeasureName>Sales Budget</MeasureName>
                      <UniqueName>[Measures].[Sales Budget]</UniqueName>
                    </ID>
                    <ItemCaption>Sales Budget</ItemCaption>
                    <BackColor>true</BackColor>

  <DataSets>
    <DataSet Name="POInfo">
      <Query>
        <DataSourceName>AdventureWorks2008R2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PONumber">
            <Value>=Parameters!PONumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
h.PurchaseOrderID 
, e.FirstName + ' ' + e.LastName Requestor
, h.OrderDate
, h.ShipDate
, h.RevisionNumber
, v.AccountNumber
, v.Name
, va.AddressLine1
, va.AddressLine2
, va.City
, va.StateProvinceName
, va.PostalCode
, va.CountryRegionName
, e.AddressLine1 ShipToAddressLine1
, e.AddressLine2 ShipToAddressLine2
, e.City ShipToCity
, e.StateProvinceName ShipToState
, e.PostalCode ShipToZip
FROM Purchasing.PurchaseOrderHeader h INNER JOIN
	Purchasing.Vendor v ON h.VendorID = v.BusinessEntityID INNER JOIN
	Purchasing.vVendorWithAddresses va ON v.BusinessEntityID = va.BusinessEntityID INNER JOIN
	HumanResources.vEmployee e ON h.EmployeeID = e.BusinessEntityID
WHERE h.PurchaseOrderID = @PONumber</CommandText>
      </Query>

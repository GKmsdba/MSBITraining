    <DataSet Name="PODetails">
      <Query>
        <DataSourceName>AdventureWorks2008R2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PONumber">
            <Value>=Parameters!PONumber.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
d.PurchaseOrderID
, d.PurchaseOrderDetailID
, ROW_NUMBER() OVER (PARTITION BY d.PurchaseOrderID ORDER BY d.PurchaseOrderDetailID) AS POLine
, d.DueDate
, d.OrderQty
, d.UnitPrice
, d.LineTotal
, p.ProductNumber
, p.Name
FROM Purchasing.PurchaseOrderDetail d INNER JOIN
	Production.Product p ON  d.ProductID = p.ProductID
WHERE d.PurchaseOrderID = @PONumber</CommandText>
      </Query>

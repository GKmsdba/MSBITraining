  <DataSets>
    <DataSet Name="PO_Outlook">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT 
h.PurchaseOrderID
, e.FirstName + ' ' + e.LastName Buyer
, DATEADD(YEAR, 3, h.OrderDate) OrderDate
, DATEADD(YEAR, 3, h.ShipDate) ShipDate
, SUM(d.LineTotal) POTotal
, SUM(d.ReceivedQty * UnitPrice) TotalReceived
, SUM(d.LineTotal) - SUM(d.ReceivedQty * UnitPrice) Remaining
FROM Purchasing.PurchaseOrderDetail d INNER JOIN
	Production.Product p ON d.ProductID = p.ProductID INNER JOIN
	Purchasing.PurchaseOrderHeader h ON d.PurchaseOrderID = h.PurchaseOrderID INNER JOIN
	Purchasing.Vendor v ON h.VendorID = v.BusinessEntityID INNER JOIN
	HumanResources.vEmployee e ON h.EmployeeID = e.BusinessEntityID 
WHERE OrderDate &lt; GETDATE()
	AND h.Status NOT IN (3, 4)
GROUP BY 
h.PurchaseOrderID,
e.FirstName + ' ' + e.LastName,
h.OrderDate,
h.ShipDate</CommandText>
      </Query>
      <Fields>
        <Field Name="PurchaseOrderID">
          <DataField>PurchaseOrderID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Buyer">
          <DataField>Buyer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ShipDate">
          <DataField>ShipDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="POTotal">
          <DataField>POTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalReceived">
          <DataField>TotalReceived</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Remaining">
          <DataField>Remaining</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ShipMonth">
          <Value>=LEFT(UCase(MONTHNAME(MONTH(Fields!ShipDate.Value))), 3)</Value>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=CDbl(Fields!Remaining.Value)</FilterExpression>
          <Operator>GreaterThan</Operator>
          <FilterValues>
            <FilterValue DataType="Float">0</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
